{
  "name": "node-ollama-telegram-agent-swarm",
  "type": "module",
  "module": "build/index.mjs",
  "source": "src/index.ts",
  "types": "./types.d.ts",
  "exports": {
    "types": "./types.d.ts",
    "import": "./build/index.mjs",
    "default": "./build/index.mjs"
  },
  "scripts": {
    "test": "dotenv -e .env -- npm run build && node -e \"process.env.CC_EXECUTE_TEST = 1; import('./build/index.mjs')\"",
    "start": "dotenv -e .env -- npm run build && node ./build/index.mjs",
    "start:repl": "dotenv -e .env -- npm run build && node -e \"globalThis[Symbol.for('error-handler-installed')] = 1; import('./scripts/repl.mjs')\" --interactive",
    "start:console": "dotenv -e .env -- npm run build && node -e \"process.env.CC_WWWROOT_PORT = -1; import('./scripts/console.mjs')\"",
    "start:debug": "dotenv -e .env -- npm run build && node --inspect-brk ./build/index.mjs",
    "build": "kill-port 80 && dotenv -e .env -- rollup -c",
    "build:docs": "rimraf docs && mkdir docs && touch docs/.gitkeep && node ./scripts/dts-docs.cjs ./types.d.ts ./docs",
    "docs:gpt": "node ./scripts/gpt-docs.mjs",
    "postinstall": "node ./scripts/postinstall.mjs",
    "ollama:dump:win": ".\\scripts\\windows\\dump_ollama.bat",
    "ollama:create:win": ".\\scripts\\windows\\create_ollama.bat",
    "ollama:dump": "sh ./scripts/windows/dump_ollama.bat",
    "ollama:create": "sh ./scripts/windows/create_ollama.bat",
    "kill": "kill-port 80"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.6",
    "@types/lodash-es": "4.17.12",
    "@types/node": "22.10.3",
    "@types/uuid": "10.0.0",
    "@types/xml2js": "0.4.14",
    "dotenv-cli": "7.4.2",
    "glob": "11.0.1",
    "kill-port": "2.0.1",
    "rimraf": "6.0.1",
    "rollup": "3.29.4",
    "rollup-plugin-dts": "6.1.1",
    "rollup-plugin-inject-process-env": "1.3.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "touch-cli": "0.0.1",
    "tslib": "2.7.0"
  },
  "dependencies": {
    "@hono/node-server": "1.13.7",
    "@hono/node-ws": "1.0.4",
    "@tensorflow/tfjs-backend-wasm": "4.22.0",
    "@tensorflow/tfjs-core": "4.22.0",
    "agent-swarm-kit": "^1.0.34",
    "di-factory": "1.0.9",
    "di-kit": "1.0.12",
    "di-scoped": "1.0.11",
    "di-singleton": "1.0.5",
    "functools-kit": "1.0.69",
    "get-moment-stamp": "1.1.1",
    "gpt4all": "4.0.0",
    "hono": "4.6.13",
    "ioredis": "5.4.2",
    "lodash-es": "4.17.21",
    "mongoose": "8.9.3",
    "ollama": "0.5.11",
    "pinolog": "1.0.2",
    "tape": "5.9.0",
    "telegraf": "4.15.3",
    "uuid": "11.0.3",
    "worker-testbed": "1.0.5",
    "xml2js": "0.6.2",
    "zod": "3.24.1"
  }
}
